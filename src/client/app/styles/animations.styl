anim-ripple-keyframes()
  0%
    opacity 0
    transform scale(.45)

  40%
    opacity .5
    transform scale(.75)

  80%
    opacity 0.1
    transform scale(.95)

  100%
    opacity 0
    transform scale(1)



@-ms-keyframes anim-ripple
  anim-ripple-keyframes()

@-webkit-keyframes anim-ripple
  anim-ripple-keyframes()

@keyframes anim-ripple
  anim-ripple-keyframes()
